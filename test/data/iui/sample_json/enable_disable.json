{
	"id": "window",
	"label": "I am a test for enabling and disabling",
	"height": 600,
	"width": 700,
	"modelName":"Enable/disable test",
	"elements":[
	{
		"id": "box_1",
		"type": "checkbox",
		"label": "checkbox 1",
		"defaultValue": false,
		"required": false
	},
	{
		"id": "list_1",
		"type":"list",
		"elements":[
		{	
			"id": "text_1",
			"type": "text",
			"width": 70,
			"validText": "-?[0-9]+",
			"dataType": "int",
			"label": "Text box 1",
			"enabledBy": "box_1",
			"helpText": "I am enabled by checkbox 1"
		},
		{
			"id": "text_2",
			"type": "text",
			"width": 70,
			"validText": "-?[0-9]+",
			"dataType": "int",
			"label": "Text box 2",
			"disabledBy": "box_1",
			"helpText": "I am disabled by checkbox 1"
		}
		]
	}
	]
}

{
    "id" : "window",
    "label": "Blue Carbon Preprocessor",
    "targetScript" : "invest_natcap.blue_carbon.blue_carbon_preprocessor",
    "modelName" : "blue_carbon_preprocessor",
    "localDocURI": "../documentation/blue_carbon_preprocessor.html",
    "height": 600,
    "width" : 700,
    "elements": [
                    {
                    "id" : "workspace_list",
                    "type": "list",
                            "elements": [
                                            {
                                                "id": "workspace_dir",
                                                "args_id": "workspace_dir",
                                                "type": "folder",
                                                "label": "Workspace",
                                                "defaultValue": "../BlueCarbon/output",
                                                "helpText": "The directory to hold output from a particular model run. <p>For more information see <a href=\"../documentation/blue_carbon.html#data-needs\">Data Needs</a>",
                                                "required": true
                                            }
                                        ]
                    },
                    {
                        "id": "lulc",
                        "type": "multi",
                        "args_id": "lulc",
                        "label": "LULC",
                        "sampleElement": {"id": "sample_id",
                                          "type": "file",
                                          "label": "Input",
                                          "validateAs": {"type": "GDAL"}},
                        "defaultValue": ["../BlueCarbon/input/GBJC_2004_mean.tif", "../BlueCarbon/input/GBJC_2050_mean.tif", "../BlueCarbon/input/GBJC_2100_mean.tif"],
                        "linkText": "Add Another",
                        "helpText": ""
                    },
                    {
                        "id": "labels_list",
                        "type": "list",
                        "elements": [
                            {
                                "id": "labels",
                                "type": "file",
                                "args_id": "labels",
                                "label": "Labels",
                                "defaultValue": "../BlueCarbon/input/carbon.csv",
                                "helpText": "",
                                "required": false
                            },
                            {
                                "id": "lulc_id",
                                "args_id": "lulc_id",
                                "type": "OGRFieldDropdown",
                                "label": "LULC ID",
                                "enabledBy": "labels",
                                "defaultValue": 1,
                                "helpText": "",
                                "returns": "strings",
                                "required": true
                            },
                            {
                                "id": "lulc_name",
                                "args_id": "lulc_name",
                                "type": "OGRFieldDropdown",
                                "label": "LULC Name",
                                "enabledBy": "labels",
                                "defaultValue": 1,
                                "helpText": "",
                                "returns": "strings",
                                "required": true
                            }
                        ]
                    }
                ]
}

                    {
                      "id": "nutrition_table",
                      "args_id": "nutrition_table",
                      "type": "file",
                      "label": "Nutrition File (CSV)",
                      "defaultValue": "../CropProduction/input/cropNutrient.csv",
                      "validateAs": {
                        "type": "CSV"
                      },
                      "required": true,
                      "enabledBy": "calculate_nutrition"
                    }
                  ]
                }
              ]
            },
            {
              "id": "calculate_valuation",
              "args_id": "calculate_valuation",

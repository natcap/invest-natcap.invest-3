{
  "id": "window",
  "label": "Crop Production",
  "targetScript": "invest_natcap.crop_production.crop_production",
  "modelName": "crop_production",
  "localDocURI": "../documentation/crop_production.html",
  "condenseElements": true,
  "height": 800,
  "width": 700,
  "include_meta": true,
  "elements": [
    {
      "id": "objectiveTabGroup",
      "type": "tabbedGroup",
      "elements": [
        {
          "id": "main_tab",
          "label": "Inputs",
          "type": "tab",
          "elements": [
            {
              "id": "alpha_only",
              "label": "This tool is in an ALPHA testing stage and should not be used for decision making.",
              "type": "label",
              "helpText": "Alpha tools are considered unstable."
            },
            {
              "id": "main_tab_list",
              "type": "list",
              "elements": [
                {
                  "id": "workspace_dir",
                  "args_id": "workspace_dir",
                  "type": "folder",
                  "label": "Workspace",
                  "defaultValue": "../CropProduction/output",
                  "required": true,
                  "helpText": "The selected folder is used as the workspace where all intermediate and output files will be written.<br><br>If the selected folder does not exist, it will be created.<br><br><b>If datasets already exist in the selected folder, they will be overwritten.</b>"
                },
                {
                  "id": "crop_file_name",
                  "args_id": "crop_file_name",
                  "type": "file",
                  "label": "Crop Map (Raster)",
                  "defaultValue": "../CropProduction/input/lulc_samp_cur.tif",
                  "validateAs": {
                    "type": "GDAL"
                  },
                  "required": true,
                  "helpText": "A GDAL-supported raster file."
                },
                {
                  "id": "reclass_table",
                  "args_id": "reclass_table",
                  "type": "file",
                  "label": "Crop Reclass Table (CSV)",
                  "defaultValue": "../CropProduction/input/reclass_table.csv",
                  "validateAs": {
                    "type": "CSV"
                  },
                  "required": true
                },
                {
                  "id": "enable_tab_existing",
                  "args_id": "enable_tab_existing",
                  "type": "checkbox",
                  "label": "Existing Yields",
                  "defaultValue": true
                }
              ]
            },
            {
              "id": "enable_tab_percentile",
              "args_id": "enable_tab_percentile",
              "type": "container",
              "label": "Percentile Yields",
              "defaultValue": false,
              "collapsible": true,
              "elements": [
                {
                  "id": "percentile_list",
                  "type": "list",
                  "elements": [
                    {
                      "id": "income_raster",
                      "args_id": "income_raster",
                      "type": "file",
                      "label": "Income Raster",
                      "defaultValue": "../CropProduction/input/OECDIncomeData.tif",
                      "validateAs": {
                        "type": "GDAL"
                      },
                      "required": true,
                      "enabledBy": "calculate_nutrition"
                    }
                  ]
                }
              ]
            },
            {
              "id": "calculate_nutrition",
              "args_id": "calculate_nutrition",
              "type": "container",
              "label": "Calculate Nutrition",
              "collapsible": true,

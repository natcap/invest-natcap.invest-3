                      "validateAs": {
                        "type": "CSV"
                      },
                      "required": true,
                      "enabledBy": "calculate_valuation",
                      "helpText": "A CSV file."
                    },
                    {
                      "id": "field_machine",
                      "args_id": "field_machine",
                      "type": "dropdown",
                      "label": "Machine Cost",
                      "options": [
                        "low_mach",
                        "actual_mach",
                        "high_mach"

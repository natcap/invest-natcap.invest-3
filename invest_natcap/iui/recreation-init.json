{
    "id" : "window",
    "label": "Recreation Model - Initial Run",
    "targetScript" : "invest_natcap.recreation.recreation_client",
    "modelName" : "recreation",
    "localDocURI": "",
    "height": 600,
    "width" : 700,
    "elements": [
            {
            "id" : "workspace_list",
            "type": "list",	        
		    "elements": [
		            {
		            "id" : "aoiFileName",
		            "args_id" : "aoiFileName",
		            "type": "file",
		            "label": "Area of Interest",
		            "defaultValue": "",
		            "validateAs": {"type": "OGR"},
		            "required": true
		        },
		        {
		            "id" : "cellSize",
		            "args_id" : "cellSize",
		            "type": "text",
		            "label": "Cell size",
		            "defaultValue": "",
		            "validText": "[0-9]+(\\.[0-9][0-9])?",
		            "dataType": "float",
		            "required": true
		        },
				{
				    "id": "cellUnit",
				    "args_id" : "cellUnit",
				    "type": "dropdown",
				    "label": "Cell unit",
				    "options":["kilometer", "meter", "mile", "foot"],
				    "returns": {"mapValues" : {"kilometer" : 1000, "meter" : 1, "mile":1609.344, "foot" : 0.3048}},
				    "defaultValue": 0
				},
		        {
		        	"id" : "workspace_dir",
		        	"args_id" : "workspace_dir",
					"type": "folder",
                	"label": "Workspace",
                	"required": true
		        },
		        {
		        	"id" : "comments",
		        	"args_id" : "comments",
					"type": "text",
                	"label": "Comments",
                	"required": false
		        }		        
		   ]
		   }
    ]
}
                
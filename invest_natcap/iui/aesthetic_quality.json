{
    "id" : "window",
    "label": "Aesthetic Quality",
    "targetScript" : "invest_natcap.aesthetic_quality.aesthetic_quality",
    "modelName" : "aesthetic_quality",
    "localDocURI": "../documentation/aesthetic_quality.html",
    "height": 600,
    "width" : 700,
    "elements": [
            {
            "id" : "workspace_list",
            "type": "list",	        
		    "elements": [
                                {
                                    "id" : "workspace_dir",
                                    "args_id" : "workspace_dir",
                                    "type": "folder",
                                    "label": "Workspace",
                                    "required": true
                                },                        
                                {
                                    "id" : "aoi_uri",
                                    "args_id" : "aoi_uri",
                                    "type": "file",
                                    "label": "Area of Interest",
                                    "defaultValue": "",
                                    "validateAs": {"type": "OGR"},
                                    "required": true
                                },
                                {
                                    "id" : "structure_uri",
                                    "args_id" : "structure_uri",
                                    "type": "file",
                                    "label": "Structures",
                                    "defaultValue": "test/invest-data/test/data/aesthetic_quality_test_data/BC_parks.shp",
                                    "validateAs": {"type": "OGR"},
                                    "required": true
                                },
                                {
                                    "id" : "dem_uri",
                                    "args_id" : "dem_uri",
                                    "type": "file",
                                    "label": "Digital Elevation Model",
                                    "defaultValue": "",
                                    "validateAs": {"type": "GDAL"},
                                    "required": true
                                },                                
                                {
                                    "id" : "refraction",
                                    "args_id" : "refraction",
                                    "type": "text",
                                    "label": "Refractivity Coefficient",
                                    "defaultValue": "0.13",
                                    "validText": "[0-9]+(\\.[0-9][0-9])?",
                                    "dataType": "float",
                                    "required": true
                                },                                
                                {
                                    "id" : "cell_size",
                                    "args_id" : "cell_size",
                                    "type": "text",
                                    "label": "Cell Size (meters)",
                                    "defaultValue": "500",
                                    "validText": "[0-9]+(\\.[0-9][0-9])?",
                                    "dataType": "float",
                                    "required": false
                                },
                                {
                                    "id" : "pop_uri",
                                    "args_id" : "pop_uri",
                                    "type": "file",
                                    "label": "Population Raster",
                                    "defaultValue": "",
                                    "validateAs": {"type": "GDAL"},
                                    "required": true
                                },
                                {
                                    "id" : "overlap_uri",
                                    "args_id" : "overlap_uri",
                                    "type": "file",
                                    "label": "Overlap Analysis Features",
                                    "defaultValue": "",
                                    "validateAs": {"type": "OGR"},
                                    "required": false
                                }
		   ]
		   }
    ]
}
                

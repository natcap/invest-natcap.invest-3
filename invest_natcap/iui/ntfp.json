{
    "id" : "window",
    "label": "Non-Timber Forest Products",
    "targetScript" : "invest_natcap.ntfp.ntfp",
    "modelName" : "ntfp",
    "localDocURI": "../documentation/ntfp.html",
    "height": 800,
    "width" : 700,
    "elements": [
		{
			"id" : "workspace_list",
			"type": "list",	        
				"elements": [
					{
						"id": "workspace_dir",
						"args_id": "workspace_dir",
						"type": "folder",
						"label": "Workspace",
						"defaultValue": "../ntfp",
						"helpText": "The directory to hold output from a particular model run. <p>For more information see <a href=\"../documentation/blue_carbon.html#data-needs\">Data Needs</a>",
						"required": true
					},
					{
						"id" : "calc_sequestration",
						"args_id": "sequestration_checkbox",
						"type": "checkbox",
						"label": "Calculate sequestration",
						"defaultValue": false,
						"required": false
					},
					{
						"id": "lulc1_uri",
						"args_id": "lulc1_uri",
						"type": "file",
						"label": "LULC Time 1",
						"defaultValue": "../BlueCarbon/input/GBJC_2004_mean.tif",
						"helpText": "The land use land cover raster for time 1. <p>For more information see <a href=\"../documentation/blue_carbon.html#data-needs\">Data Needs</a>",
						"required": true
					}
				]
		}
	]
}
